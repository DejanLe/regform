 
<h1>Requests</h1>

<table class="table   table-hover table-responsive">
  <thead>
    <tr>
      <th>Date of request</th>
      <th>New account</th>
      <th>Change privileges</th>
      <th>Change privileges</th>
      <th>Disable account</th>
      <th>Re enable account</th>
      <th>Mc</th>
      <th>Hmfp</th>
      <th>Apg</th>
      <th>Ap hmfp</th>
      <th>Mc corporate</th>
      <th>Other</th>
      <th>Specify</th>
      <th>Client</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.date_of_request %></td>
        <td><%= request.new_account %></td>
        <td><%= request.change_privileges %></td>
        <td><%= request.change_privileges %></td>
        <td><%= request.disable_account %></td>
        <td><%= request.re_enable_account %></td>
        <td><%= request.mc %></td>
        <td><%= request.hmfp %></td>
        <td><%= request.apg %></td>
        <td><%= request.ap_hmfp %></td>
        <td><%= request.mc_corporate %></td>
        <td><%= request.other %></td>
        <td><%= request.specify %></td>
        <td><%= request.client %></td>
        <td><%= link_to 'Show', request %></td>
        <td><%= link_to 'Edit', edit_request_path(request) %></td>
        <td><%= link_to 'Destroy', request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
 
